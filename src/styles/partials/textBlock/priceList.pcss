.priceShedule {
  position: relative;
  z-index: 15;
  background-color: white;
}

.priceList {
  list-style: none;
  margin: 0;
  padding: 0;
  display: flex;
  flex-wrap: wrap;
}

.priceList-item {
  width: 25%;
  padding: 40px 0 40px 40px;
  box-sizing: border-box;

  &:nth-child(4n+1) {
    padding-left: 0;
  }
}

.priceList-title {
  color: $color-blue;
  font: $font-text;
  margin: 0;
  padding: 0;
}

.priceList-price {
  font: $font-service;
  margin: 0;
  padding: 0;
}