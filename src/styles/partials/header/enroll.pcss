.enroll {
  height: $header-height;
  box-sizing: border-box;
  padding: 0 $margin-holder;
  background-color: $color-accent;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  transition: .3s;

  &:hover {
    background-color: black;
    cursor: pointer;
  }
}

.enroll-title {
  display: inline-block;
}